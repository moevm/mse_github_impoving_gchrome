{
  "name": "GitExtension",
  "version": "1.0",
  "description": "MOEVM PI project",
  "page_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://github.com/*/*" ],
      "css": ["styles/item5.css"],
      "js": [ "contentscripts/item5.js" ]
    }
  ],
  "manifest_version": 2
}